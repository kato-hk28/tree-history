{
    "manifest_version":2,
    "name":"(Tree History)",
    "version":"0.0.1",
    "permissions":[
        "tabs",
        "history"
    ],
    "browser_action": {
        "default_icon": {
            "1": "icon1.png"
        },
        "default_title": "テスト",
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches":[
                "<all_urls>"
            ],
            "js":[
                "jquery-3.6.0.min.js",
                "content.js"
            ]
        }
    ],
    "background":{
        "scripts":[
            "background.js"
        ],
        "persistent": false
    }
}